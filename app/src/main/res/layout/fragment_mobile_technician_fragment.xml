                                    android:gravity="center"
                                    android:text="Shop# 1,2 Basement Raheem Centre Near Baghwali Masjid Vehari"
                                    android:textColor="@color/shop_address_color"
                                    android:textSize="18dp"
                                    tools:ignore="HardcodedText,SpUsage,TextSizeCheck,TextContrastCheck" />

                                <TextView
                                    android:id="@+id/mobile_technician1.txt3"
                                    android:layout_width="226dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="3dp"
                                    android:layout_marginEnd="25dp"
                                    android:fontFamily="@font/oswald_bold"
                                    android:gravity="center"
                                    android:text="M. Uzair Manzoor"
                                    android:textColor="@color/appcolor"
                                    android:textSize="20dp"
                                    tools:ignore="HardcodedText,SpUsage,TextSizeCheck,TextContrastCheck" />
                            </LinearLayout>
                        </LinearLayout>

