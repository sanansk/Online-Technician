                            android:id="@+id/mobile_technician1.txt1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentEnd="true"
                            android:fontFamily="@font/oswald_bold"
                            android:gravity="center"
                            android:text="Uzair Mobile Shop"
                            android:textColor="@color/shop_name_color"
                            android:textSize="22dp"
                            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,TextContrastCheck" />

                        <LinearLayout
                            android:id="@+id/mobile_technician1.main_section1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/mobile_technician1.txt1"
                            tools:ignore="ExtraText">

                            <ImageView
                                android:id="@+id/mobile_technician1_img"
                                android:layout_width="125dp"
                                android:layout_height="147dp"
                                android:layout_marginStart="4dp"
                                app:srcCompat="@drawable/mobile_tech1_img"
                                tools:ignore="ContentDescription,ImageContrastCheck" />

